class Main inherits IO
    print Int x :
        local String s; end;
        s = if x < 0 then
                local String m = "Minus"; end;
                m + -x;
            else
                m = "";
                m + x;
            end
        ;
        [out s];              
    end;
    
    main :
        [print -4];
        [print 25];
    end;
end;
