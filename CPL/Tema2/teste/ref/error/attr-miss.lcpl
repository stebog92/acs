class intModel
   var Int value; end;
   setValue Int val : self.val = val; end;
end;

class int inherits intModel
   getValue -> Int : value; end;
end;

class Main inherits IO
    mac Int a, Int b, int c -> Int : a * b + [c.getValue]; end;
    main :
        local intModel x = new int; end;
        [x.setValue 3];
        [out [mac 1, 2, {int x}]];
    end;
end;
