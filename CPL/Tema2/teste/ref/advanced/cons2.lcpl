class Cons 
    var Int xcar; Cons xcdr; end;
    
    xcdr -> Cons: xcdr; end;
    xcar -> Int: xcar; end;
    size -> Int:
        local Int size; Cons c = self; end;
        while !(c == void) loop
           size = size + 1;
           [(new IO).out [c.xcar] + " "];           
           c = [c.xcdr];
        end;  
        size;      
    end;

    init Int hd, Cons tl -> Cons :
        xcar = hd;
        xcdr = tl;
        self;
    end;
end;

class Main
    newCons Int hd, Cons tl -> Cons :
    local
        Cons c = new Cons;
    end;
        [c.init hd, tl];
    end;

    main:
        local 
            Cons c = [newCons 10,[newCons 20,[newCons 30, [newCons 40, void]]]]; 
        end;
        [(new IO).out [c.size]];
    end;
end;
